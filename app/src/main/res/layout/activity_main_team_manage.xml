<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

  <data>

  </data>

  <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      tools:context=".ui.team.manage.MainTeamManageActivity">

    <com.heechan.membeder.ui.view.header.NavigationHeader
        android:id="@+id/hd_teamBuilding"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:navigationTitle="팀 관리" />


    <TextView
        android:id="@+id/txt_membermanage_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/hd_teamBuilding"
        android:textSize="20sp"
        android:text="멤버 관리"
        android:textFontWeight="700"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="32sp" />

    <TextView
        android:id="@+id/txt_membermanage_memberrecruitment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_membermanage_title"
        android:textSize="20sp"
        android:text="멤버 모집"
        android:textFontWeight="500"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="27sp" />


    <ImageView
        android:layout_marginEnd="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:src="@drawable/ic_arrow_right"

        app:layout_constraintTop_toTopOf="@id/txt_membermanage_memberrecruitment"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintBottom_toBottomOf="@id/txt_membermanage_memberrecruitment"

        />

    <TextView
        android:id="@+id/txt_membermanage_memberban"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_membermanage_memberrecruitment"
        android:textSize="20sp"
        android:text="멤버 추방"
        android:textFontWeight="500"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="27sp" />


    <ImageView
        android:layout_marginEnd="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:src="@drawable/ic_arrow_right"

        app:layout_constraintTop_toTopOf="@id/txt_membermanage_memberban"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintBottom_toBottomOf="@id/txt_membermanage_memberban" />

    <TextView
        android:id="@+id/txt_membermanage_authority"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_membermanage_memberban"
        android:textSize="20sp"
        android:text="권한 관리"
        android:textFontWeight="500"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="27sp" />


    <ImageView
        android:layout_marginEnd="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:src="@drawable/ic_arrow_right"

        app:layout_constraintTop_toTopOf="@id/txt_membermanage_authority"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintBottom_toBottomOf="@id/txt_membermanage_authority" />

    <TextView
        android:id="@+id/txt_caleandermanage_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_membermanage_authority"
        android:textSize="20sp"
        android:text="일정 관리"
        android:textFontWeight="700"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="32sp" />

    <TextView
        android:id="@+id/txt_caleandermanage_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_caleandermanage_title"
        android:textSize="20sp"
        android:text="할일 수정"
        android:textFontWeight="500"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="27sp" />


    <ImageView
        android:layout_marginEnd="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:src="@drawable/ic_arrow_right"

        app:layout_constraintTop_toTopOf="@id/txt_caleandermanage_edit"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintBottom_toBottomOf="@id/txt_caleandermanage_edit"

        />

    <TextView
        android:id="@+id/txt_caleandermanage_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_caleandermanage_edit"
        android:textSize="20sp"
        android:text="할일 삭제"
        android:textFontWeight="500"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="27sp" />


    <ImageView
        android:layout_marginEnd="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:src="@drawable/ic_arrow_right"

        app:layout_constraintTop_toTopOf="@id/txt_caleandermanage_delete"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintBottom_toBottomOf="@id/txt_caleandermanage_delete" />
    <TextView
        android:id="@+id/txt_onother_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_caleandermanage_delete"
        android:textSize="20sp"
        android:text="그 외"
        android:textFontWeight="700"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="32sp" />
    <TextView
        android:id="@+id/txt_other_profileEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/txt_onother_title"
        android:textSize="20sp"
        android:text="팀 프로필 수정"
        android:textFontWeight="500"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24sp"
        android:layout_marginTop="27sp" />


    <ImageView
        android:layout_marginEnd="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:src="@drawable/ic_arrow_right"

        app:layout_constraintTop_toTopOf="@id/txt_other_profileEdit"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintBottom_toBottomOf="@id/txt_other_profileEdit" />

  </androidx.constraintlayout.widget.ConstraintLayout>
</layout>